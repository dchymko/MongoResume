@using MongoResume.Models
@model IEnumerable<Resume<WorkExperience>>

@{
	ViewBag.Title = "Index";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Thingies</h2>
<ul>
@foreach (var resume in Model)
{
	<li>
		@Html.Raw(resume.ResumeName.ToString())
	</li>
}
</ul>
@Html.ActionLink("new", "New")<br />

@Html.ActionLink("Print Resume", "PrintResume" )
<a href="http://www.web2pdfconvert.com/convert">Save to PDF</a>